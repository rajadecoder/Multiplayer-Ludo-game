<?xml version='1.0' encoding='utf-8'?>
<widget id="com.SpotDoit.Railway" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Fullscreen">
        <param name="android-package" value="com.filfatstudios.fullscreen.Fullscreen" />
    </feature>
    <feature name="AdMob">
        <param name="android-package" value="com.cupertino.cordova.plugin.AdMob" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>SpotDoit-Railway-Preparation</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="SpotDoit@gmail.com" href="http://spotdoit.in">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="http://*/*" launch-external="yes" />
    <allow-intent href="https://*/*" launch-external="yes" />
    <allow-navigation href="https://firebasestorage.googleapis.com/v0/b/spotdoit14812.appspot.com/o/*" />
    <icon density="mdpi" height="57" platform="android" src="res\icon\android\spot3.png" width="57" />
    <allow-intent href="market:*" />
    <splash density="land-hdpi" src="res/screen/android/screen-hdpi-landscape.png" />
    <splash density="land-ldpi" src="res/screen/android/screen-xhdpi-portrait.png" />
    <splash density="land-mdpi" src="res/screen/android/screen-xhdpi-landscape.png" />
    <splash density="land-xhdpi" src="res/screen/android/screen-mdpi-landscape.png" />
    <splash density="port-hdpi" src="res/screen/android/screen-hdpi-portrait.png" />
    <splash density="port-ldpi" src="res/screen/android/screen-ldpi-portrait.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
